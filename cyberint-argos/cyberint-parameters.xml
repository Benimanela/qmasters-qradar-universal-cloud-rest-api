<?xml version="1.0" encoding="UTF-8" ?>
<WorkflowParameterValues xmlns="http://qradar.ibm.com/UniversalCloudRESTAPI/WorkflowParameterValues/V2">

    <!-- "hostName" - CyberInt API host. (required) -->
	<Value name="hostName" value="https://<instance name>.cyberint.io"/>
    <!-- "apiKey" - CyberInt API key for QRadar. (required) -->
	<Value name="apiKey" value="<your api key>"/>

    <!-- "severity" - Cyberint alert severity, can be multiple separated by commas, allowed values: "very_high", "high", "medium", "low". (optional) -->
	<Value name="severity" value='"very_high", "high", "medium", "low"'/>
	<!-- "status" - Show closed/open alerts, allowed values: "open", "acknowledged", "closed" . (optional) -->
	<Value name="status" value='"open", "acknowledged"'/>

	<!-- "pageSize" - Number of alerts per page, default = 100. (optional) -->
	<Value name="pageSize" value="100"/>
	<!-- "timeZone" - select your time zone (Default=UTC). (optional) -->
	<Value name="timeZone" value="UTC"/>
</WorkflowParameterValues>
