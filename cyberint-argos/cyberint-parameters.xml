<?xml version="1.0" encoding="UTF-8" ?>
<WorkflowParameterValues xmlns="http://qradar.ibm.com/UniversalCloudRESTAPI/WorkflowParameterValues/V2">

    <!-- "hostname" - CyberInt API host. (required) -->
	<Value name="hostname" value="https://<instance name>.cyberint.io"/>
    <!-- "api_key" - CyberInt API key for QRadar. (required) -->
	<Value name="api_key" value="<your api key>"/>

    <!-- "severity" - Cyberint alert severity, can be multiple separated by commas, allowed values: "very_high", "high", "medium", "low". (optional) -->
	<Value name="severity" value='"very_high", "high", "medium", "low"'/>
	<!-- "is_closed" - Show closed/open alerts, default value: "open", "acknowledged", "closed" . (optional) -->
	<Value name="status" value='"open", "acknowledged", "closed"'/>

	<!-- "page_size" - Number of alerts per page, default = 100. (optional) -->
	<Value name="page_size" value="100"/>
</WorkflowParameterValues>